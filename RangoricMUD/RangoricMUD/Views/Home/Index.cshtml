@using System.Web.Optimization
@model RangoricMUD.Controllers.Models.IndexModel
@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge" /> 
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width"/>
        <title>@ViewBag.Title</title>
        <link type="text/css" rel="stylesheet" href="~/Assets/Styles.css"/>
        @Scripts.Render("~/Assets/Scripts/LibraryScripts")
        <script src="/signalR/hubs" type="text/javascript"> </script>
        @Scripts.Render("~/Assets/Scripts/OtherScripts")
    </head>
    <body>
        <div id="Interface">
            <div data-bind="template:{ name: 'Button', foreach:Buttons}"></div>            
            <div></div>
        </div>
        <div id="Scene"></div>
        @Html.Partial("../Shared/Components/Buttons")
        @Html.Partial("../Shared/Components/Panels")
        @Html.Partial("../Shared/Components/Forms")
    </body>
</html>


