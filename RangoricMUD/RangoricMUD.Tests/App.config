<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="Accounts/Admin/Name" value="Admin"/>
    <add key="Accounts/Admin/Email" value="test@email.com"/>
  </appSettings>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="SpecifiedPickupDirectory" from="test@email.com">
        <specifiedPickupDirectory pickupDirectoryLocation="C:/Emails"/>
      </smtp>
    </mailSettings>
  </system.net>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
